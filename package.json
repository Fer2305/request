  "license": "MIT",
  "scripts": {
    "test": "env NODE_ENV=testing jest --forceExit --detectOpenHandles --silent",
    "test:debug": "env NODE_ENV=testing jest --forceExit --detectOpenHandles",
    "make-badges": "istanbul-badges-readme"
  },
  "devDependencies": {
    "codecov": "^3.8.1",
    "istanbul-badges-readme": "^1.2.1",
